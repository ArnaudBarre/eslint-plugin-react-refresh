{
  "name": "eslint-plugin-react-refresh",
  "version": "0.3.5",
  "license": "MIT",
  "scripts": {
    "build": "scripts/bundle.ts",
    "prettier": "bun prettier-ci --write",
    "prettier-ci": "prettier --ignore-path=.gitignore --check '**/*.{ts,json,md,yml}'",
    "test": "src/tests.ts",
    "ci": "tsc && bun prettier-ci && bun test && bun run build"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "devDependencies": {
    "@nabla/tnode": "^0.9.0",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/utils": "^5.59.1",
    "bun-types": "^0.5.8",
    "eslint": "^8.39.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
